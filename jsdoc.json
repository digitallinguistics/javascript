{
  "opts": {
    "destination": "docs",
    "readme": "README.md",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "exclude": [],
    "excludePattern": "\\.test\\.",
    "include": ["src"]
  },
  "sourceType": "module",
  "templates": {
    "theme": "cosmo"
  }
}
